{
    "name": "leskis/cookie-consent",
    "description": "Easily add Cookie Consent script from Silktide. A free solution to the EU Cookie Law.",
    "type": "bolt-extension",
    "keywords": ["cookie","Silktide","EU Cookie Law"],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Béhier-Dévigne",
            "homepage": "http://www.leskis.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\Leskis\\CookieConsent\\": "src/"
        }
    },

    "extra": {
        "bolt-assets": "assets",
        "bolt-icon": "assets/bolt-icon.png",
        "bolt-class": "Bolt\\Extension\\Leskis\\CookieConsent\\CookieConsentExtension"
    }
}
